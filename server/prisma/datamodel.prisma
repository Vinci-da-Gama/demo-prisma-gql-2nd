type Post {
  id: ID! @id
  published: Boolean! @default(value: false)
  title: String!
  content: String!
}

type Course {
  id: ID! @id
  isPublished: Boolean! @default(value: false)
  name: String!
  description: String!
}

type User {
  id: ID! @id
  email: String! @unique
  password: String!
}
